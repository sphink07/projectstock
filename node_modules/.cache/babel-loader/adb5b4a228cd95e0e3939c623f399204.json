{"ast":null,"code":"import Swal from \"sweetalert2\";\nexport default {\n  data() {\n    return {\n      StocK1: [],\n      ID: \"\",\n      number: 0\n    };\n  },\n\n  mounted() {\n    this.axios.get(\"http://localhost:3000/stock\").then(response => {\n      this.StocK1 = response.data;\n      console.log(response.data);\n    });\n  },\n\n  methods: {\n    DeleteItem(codeid) {\n      this.axios.delete(`http://localhost:3000/delete`, {\n        data: {\n          id: codeid\n        }\n      }).then(response => {\n        console.log(response);\n        window.location.reload();\n      }).catch(function (error) {\n        document.write(\"ส่งข้อมูลไม่สำเร็จ\" + \"<br>\" + \"สาเหตุ :\" + error);\n      });\n    },\n\n    AlertClick(list) {\n      this.number = list;\n      let codeid = this.number;\n      Swal.fire({\n        title: \"WARNING !!!\",\n        text: \"ยืนยันที่จะลบหรือไม่?\",\n        icon: \"warning\",\n        showCancelButton: true,\n        confirmButtonColor: \"#3085d6\",\n        cancelButtonColor: \"#d33\",\n        confirmButtonText: \"ตกลง\",\n        cancelButtonText: \"ยกเลิก\"\n      }).then(result => {\n        if (result.isConfirmed) {\n          Swal.fire(\"ลบสำเร็จ!\", \"ข้อมูลใน stock ถูกลบแล้ว\", \"success\");\n          this.DeleteItem(codeid);\n        }\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAoEA;AACA;EACAA;IACA;MACAC,UADA;MAEAC,MAFA;MAGAC;IAHA;EAKA,CAPA;;EASAC;IACA;MACA;MACAC;IACA,CAHA;EAIA,CAdA;;EAeAC;IACAC;MACA,WACAC,MADA,CACA,8BADA,EACA;QAAAR;UAAAS;QAAA;MAAA,CADA,EAEAC,IAFA,CAEAC;QACAN;QACAO;MACA,CALA,EAMAC,KANA,CAMA;QACAC;MACA,CARA;IASA,CAXA;;IAYAC;MACA;MACA;MACAC;QACAC,oBADA;QAEAC,6BAFA;QAGAC,eAHA;QAIAC,sBAJA;QAKAC,6BALA;QAMAC,yBANA;QAOAC,yBAPA;QAQAC;MARA,GASAd,IATA,CASAe;QACA;UACAT;UACA;QACA;MACA,CAdA;IAeA;;EA9BA;AAfA","names":["data","StocK1","ID","number","mounted","console","methods","DeleteItem","delete","id","then","response","window","catch","document","AlertClick","Swal","title","text","icon","